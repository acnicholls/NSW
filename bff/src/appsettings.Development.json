{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "NSW": "Trace"
    }
  },
  "AllowedHosts": "*",
  "Ssl": {
    "keyLocation": "/ssl/nsw.key",
    "certLocation": "/ssl/nsw.crt",
    "httpPort": 5004,
    "httpsPort": 5005
  },
  "Authentication": {
    "LoggedInRedirect": "https://localhost:3000/loggedin",
    "InternalAddressPart": "http://localhost:5006",
    "ExternalAddressPart": "https://localhost:5007",
    "Authority": "https://localhost:5007",
    "ClientId": "NSW.Bff",
    "ClientSecret": "secret",
    "MetadataAddress": "https://localhost:5007/.well-known/openid-configuration",
    "RequireHttpsMetadata": true,
    // "CallbackPath": "http://bff:5004/signin-oidc",
    "ResponseType": "code",
    "GetClaimsFromUserInfoEndpoint": true,
    "SaveTokens": true
  },
  "Api": {
    "BaseUrl": "https://localhost:5003"
  }
}