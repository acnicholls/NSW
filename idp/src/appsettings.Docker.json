{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information",
      "NSW": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ConnectionString": "DockerConnection",
  "Authentication": {
    "LoggedInRedirect": "https://localhost/loggedin",
    "LoggedOutRedirect": "https://localhost",
    "InternalAddressPart": "idp:5006",
    "ExternalAddressPart": "localhost",
    "Authority": "https://localhost",
    "ClientId": "NSW.Bff",
    "ClientSecret": "",
    "MetadataAddress": "http://idp:5006/.well-known/openid-configuration",
    "RequireHttpsMetadata": false,
    "ResponseType": "code",
    "GetClaimsFromUserInfoEndpoint": true,
    "SaveTokens": true
  },
  "Api": {
    "BaseUrl": "https://api:5003"
  },
  "Ssl": {
    "keyLocation": "/ssl/nsw.key",
    "certLocation": "/ssl/nsw.crt",
    "httpPort": 5006,
    "httpsPort": 5007
  }
}