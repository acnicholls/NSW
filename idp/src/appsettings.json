{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information",
      "NSW": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ConnectionString": "DefaultConnection",
  "Authentication": {
    "LoggedInRedirect": "https://localhost:3000/loggedin",
