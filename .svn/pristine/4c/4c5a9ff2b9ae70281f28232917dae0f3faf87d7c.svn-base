<%@ Master Language="C#" AutoEventWireup="True" CodeBehind="Mobile.Master.cs" Inherits="NSW.MobileMaster" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<head runat="server">
    <title>Mikechi.com</title>
    <link href="~/Styles/MobileSite.css" rel="stylesheet" type="text/css" />
    <meta name="viewport" content="width=device-width"/>
    <asp:ContentPlaceHolder ID="HeadContent" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form runat="server" id="NSW_Form">
    <div class="page">
        <div class="header">
            <div class="title">
                <h1>
                    <a href="<%= SplashLink %>" class="noLink"><asp:Label ID="lblTitle" class="lblTitle" runat="server"></asp:Label></a>
                </h1>
            </div>
            <div class="loginDisplay">
                <asp:LoginView ID="HeadLoginView" runat="server" EnableViewState="false">
                    <AnonymousTemplate>
                        [ <a href="~/Account/Login.aspx" ID="HeadLoginStatus" runat="server"><asp:Label ID="lblLogin" class="lblLogin" runat="server"></asp:Label></a> ]
                    </AnonymousTemplate>
                    <LoggedInTemplate>
                        [ <asp:LoginStatus ID="HeadLoginStatus" runat="server" LogoutAction="Redirect" LogoutText="Logout" LogoutPageUrl="~/"/> ]
                    </LoggedInTemplate>
                </asp:LoginView>
            </div>
            <div class="clear"></div>
            <div class="MenuSection">
                <div class="NavhideSkiplink">
                    <asp:Menu ID="NavigationMenu" runat="server" CssClass="navMenu" EnableViewState="false" IncludeStyleBlock="false" Orientation="Horizontal">
                        <Items>
                            <asp:MenuItem NavigateUrl="~/Default.aspx" Text="Home"/>
                            <asp:MenuItem NavigateUrl="~/Search.aspx" Text="Search"/>
                        </Items>
                    </asp:Menu>
                </div>
            </div>
            <div class="clear"></div>
        </div>
        <div class="main">
            <asp:ContentPlaceHolder ID="MainContent" runat="server"/>
        </div>
        <div class="clear">
        </div>
    </div>
    <div class="footer">
        <asp:Label ID="lblFooter" runat="server" CssClass="footer"></asp:Label>
        <br />
        <br />
    </div>
    </form>
</body>
</html>
