/* ad css stuff */

div.container 
{
    width: 100%;
    height: 100%;
    text-align:center;
}
div.adBannerLeft
{
    display: inline-block; 
    *display: inline; 
    zoom: 1; 
    vertical-align: top; 
    font-size: 12px;
    width: 24%;
    float: left;
    text-align: right;
    padding-top: 100px;
}
div.adBannerRight
{
    display: inline-block; 
    *display: inline; 
    zoom: 1; 
    vertical-align: top; 
    font-size: 12px;
    width: 24%;
    text-align: left;
    float:right;
    padding-top: 100px;
}

div.page
{
    display: inline-block; 
    *display: inline; 
    zoom: 1; 
    vertical-align: top; 
    font-size: 12px;
    width: 50%;
    background-color: #fff;
    margin: 20px auto 0px auto;
    border: 1px solid #496077;
    float:left;
}

