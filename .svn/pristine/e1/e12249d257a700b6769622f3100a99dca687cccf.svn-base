<?xml version="1.0"?>
<configuration>

  <location path="Admin">
    <system.web>
      <authorization>
        <allow roles="Admin"/>
        <deny roles="Member"/>
      </authorization>
    </system.web>
  </location>

  <system.web>
    <authorization>
      <deny users="?"/>
    </authorization>
  </system.web>

</configuration>
