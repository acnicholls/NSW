{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "NSW": "Debug"
    }
  },
  "ConnectionString": "NSW_Development",
  "LogSproc": "insertSiteLog",
  "LogType": "file",
  "LogLocation": "App_Data/Log",
  "AllowedHosts": "*",
  "Ssl": {
    "keyLocation": "/ssl/nsw.key",
    "certLocation": "/ssl/nsw.crt",
    "httpPort": 5002,
    "httpsPort": 5003
  },
  "Authentication": {
    "LoggedInRedirect": "https://localhost:3000/loggedin",
    "InternalAddressPart": "http://localhost:5006",
    "ExternalAddressPart": "https://localhost:5007",
    "Authority": "https://localhost:5007",
    "ClientId": "NSW.Bff",
    "ClientSecret": "secret",
    "MetadataAddress": "http://localhost:5006/.well-known/openid-configuration",
    "RequireHttpsMetadata": false,
    "ResponseType": "code",
    "GetClaimsFromUserInfoEndpoint": true,
    "SaveTokens": true
  }
}