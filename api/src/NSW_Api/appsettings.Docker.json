{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information",
            "NSW": "Debug"
        }
    },
    "ConnectionString": "DockerConnection",
    "LogSproc": "insertSiteLog",
    "LogType": "file",
    "LogLocation": "App_Data/Log",
    "AllowedHosts": "*",
    "Ssl": {
        "keyLocation": "/ssl/api.key",
        "certLocation": "/ssl/api.crt",
        "httpPort": 5002,
        "httpsPort": 5003
    },
    "Authentication": {
        "Authority": "https://localhost",
        "MetadataAddress": "http://idp:5006/.well-known/openid-configuration",
        "RequireHttpsMetadata": false,
    }
}