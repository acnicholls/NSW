{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "System": "Information",
      "Microsoft": "Information",
      "NSW": "Information"
    }
  },
  "ConnectionString": "NSW_Development",
  "LogSproc": "insertSiteLog",
  "LogType": "file",
  "LogLocation": "App_Data/Log",
  "AllowedHosts": "*",
  "Ssl": {
    "keyLocation": "/ssl/api.key",
    "certLocation": "/ssl/api.crt",
    "httpPort": 5002,
    "httpsPort": 5003
  },
  "Authentication": {
    "Authority": "https://localhost:5007",
    "MetadataAddress": "https://localhost:5007/.well-known/openid-configuration",
    "RequireHttpsMetadata": true
  }
}